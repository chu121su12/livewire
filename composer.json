{
    "name": "calebporzio/livewire",
    "description": "Build and test rich client-side functionality: with only backend code.",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb Porzio",
            "email": "calebporzio@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/chu121su12/framework.git"
        }
    ],
    "require": {
        "cboden/ratchet": "^0.4.1",
        "symfony/dom-crawler": "^3.4",
        "opis/closure": "^3.1",
        "yosymfony/resource-watcher": "^2.0",
        "symfony/process": "^3.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Livewire\\LivewireServiceProvider"
            ],
            "aliases": {
                "Livewire": "Livewire\\Livewire"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Livewire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/framework": "^5.7"
    }
}
